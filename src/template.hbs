<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <title>DI RSS Feed</title>
    <link rel="stylesheet" href="./content.css">
  </head>
  <body>
    <div class="container">
      <h1 class="table-header">DI RSS Feed</h1>
      <ul>
        {{#each items}}
        <li class="table-row">
          <h2 class="col">{{this.title}}</h2>
          <div>
            <div class="col col-2">{{this.content}}</div>
            <div class="col col-3"><a href="{{this.link}}">{{this.link}}</a></div>
            <div class="col col-4">{{this.pubDate}}</div>
          </div>
          
        </li>
        {{/each}}
    </ul>
    </div>
  </body>
</html>